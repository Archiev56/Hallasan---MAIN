[node name="DialogText" type="Node" parent="NPC\'s/Sansin/DialogueInteraction"]
script = ExtResource("9_oy0af")
text = "BOY [shake] *COUGH* [/shake] BOY! I NEED YOU! [shake] *COUGH* [/shake] [shake] *COUGH* [/shake]"
npc_info = ExtResource("10_qgauo")

[node name="DialogText6" type="Node" parent="NPC\'s/Sansin/DialogueInteraction"]
script = ExtResource("9_oy0af")
text = "Sanwoo… you went where you should not. The mountain has shown you its shadow."
npc_info = ExtResource("10_qgauo")

[node name="DialogText4" type="Node" parent="NPC\'s/Sansin/DialogueInteraction"]
script = ExtResource("9_oy0af")
text = "Grandfather… what was that thing?"
npc_info = ExtResource("10_qgauo")

[node name="DialogText8" type="Node" parent="NPC\'s/Sansin/DialogueInteraction"]
script = ExtResource("9_oy0af")
text = "Do not speak of it. Not yet. My strength fades, and the weight of Hallasan is too heavy for old bones."
npc_info = ExtResource("10_qgauo")

[node name="DialogText10" type="Node" parent="NPC\'s/Sansin/DialogueInteraction"]
script = ExtResource("9_oy0af")
text = "You must go down the mountain, Sanwoo.  Learn its ways, you have one year. Only then can you reseal that monster. "
npc_info = ExtResource("10_qgauo")

[node name="DialogText9" type="Node" parent="NPC\'s/Sansin/DialogueInteraction"]
script = ExtResource("9_oy0af")
text = "But what about you? You can barely rise from bed—"
npc_info = ExtResource("10_qgauo")

[node name="DialogText2" type="Node" parent="NPC\'s/Sansin/DialogueInteraction"]
script = ExtResource("9_oy0af")
text = "Do not worry for me. The tiger still keeps watch — my last, truest friend. He will see me through."
metadata/_custom_type_script = "uid://cwdm0l0hkothu"

[node name="DialogText3" type="Node" parent="NPC\'s/Sansin/DialogueInteraction"]
script = ExtResource("9_oy0af")
text = "Before you leave, visit your grandmother’s shrine. She will guide your steps better than I can now."
metadata/_custom_type_script = "uid://cwdm0l0hkothu"

[node name="DialogText5" type="Node" parent="NPC\'s/Sansin/DialogueInteraction"]
script = ExtResource("9_oy0af")
text = "Go, Sanwoo. The mountain waits."
metadata/_custom_type_script = "uid://cwdm0l0hkothu"

