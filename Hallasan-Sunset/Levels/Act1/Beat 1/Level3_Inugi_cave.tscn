[node name="DialogText" type="Node" parent="NPC/DialogueInteraction"]
script = ExtResource("17_7fa12")
text = "Ah, the blood of Sanshin. So the cub wanders into the den of the serpent."
npc_info = ExtResource("14_0kuht")

[node name="DialogText6" type="Node" parent="NPC/DialogueInteraction"]
script = ExtResource("17_7fa12")
text = "Nine hundred and ninety-nine winters I have waited… and now only one year remains."
npc_info = ExtResource("14_0kuht")

[node name="DialogText4" type="Node" parent="NPC/DialogueInteraction"]
script = ExtResource("17_7fa12")
text = "Your grandfather rots, too weak to bind me much longer."
npc_info = ExtResource("14_0kuht")

[node name="DialogText8" type="Node" parent="NPC/DialogueInteraction"]
script = ExtResource("17_7fa12")
text = "Each dawn, each nightfall, another link to my chain breaks."
npc_info = ExtResource("14_0kuht")

[node name="DialogText9" type="Node" parent="NPC/DialogueInteraction"]
script = ExtResource("17_7fa12")
text = "When the year is done, I will rise."
npc_info = ExtResource("14_0kuht")

[node name="DialogText10" type="Node" parent="NPC/DialogueInteraction"]
script = ExtResource("17_7fa12")
text = "The mountain will split, the rivers will choke, and the sky itself will bow."
npc_info = ExtResource("14_0kuht")

[node name="DialogText11" type="Node" parent="NPC/DialogueInteraction"]
script = ExtResource("17_7fa12")
text = "Remember this night, boy, for it is the first beat of the drum that grants my freedom."
npc_info = ExtResource("14_0kuht")

